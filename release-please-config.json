{
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "go",
      "prerelease": false,
      "draft": false,
      "changelog-sections": [
        {
          "type": "feat",
          "section": "Features",
          "hidden": false
        },
        {
          "type": "fix",
          "section": "Bug Fixes",
          "hidden": false
        },
        {
          "type": "perf",
          "section": "Performance Improvements",
          "hidden": false
        },
        {
          "type": "revert",
          "section": "Reverts",
          "hidden": false
        },
        {
          "type": "chore",
          "section": "Chores",
          "hidden": false
        },
        {
          "type": "build",
          "section": "Build System",
          "hidden": false
        }
      ]
    },
    "stages/helm-promise": {
      "changelog-path": "CHANGELOG.md",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "include-component-in-tag": true,
      "draft": false,
      "prerelease": false
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
